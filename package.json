{
  "name": "appocar",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "npm --workspace appocar-web run dev",
    "build:web": "npm --workspace appocar-web run build",
